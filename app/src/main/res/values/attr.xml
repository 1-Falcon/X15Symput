<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GlobalAttrDeclare">
        <attr name="subLabel" format="string" />
        <attr name="keyBackground" format="reference" />
        <attr name="keyTextColor" format="color" />
        <attr name="keyTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="GwaKeyboardKey">
        <attr name="subLabel" />
    </declare-styleable>

    <declare-styleable name="CustomKeyboardView">
        <attr name="keyTextColor" />
        <attr name="keyTextSize" />
        <attr name="keyBackground" />
    </declare-styleable>

    <declare-styleable name="CustomKey">
        <attr name="subLabel" />
        <attr name="isChangeLanguageKey" format="boolean" />
        <attr name="keyTextSize" />
    </declare-styleable>
</resources>