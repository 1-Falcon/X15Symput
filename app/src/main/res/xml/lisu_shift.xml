<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:keyHeight="@dimen/key_height">

    <!--[["\'", "ꓼ", "ꓱ", "ꓤ", "ꓕ", "ꓻ", "ꓵ", "꓾", "ˍ", "ꓒ"],-->
    <!--["ꓯ", "ꓸꓼ", "ꓷ", "ꓞ", "ꓨ", "ꓺ", "ꓩ", "ꓘ", "ꓶ"],-->
    <!--["unshift","ꓹ", ":", "ꓛ", "ꓥ", "ꓭ", "-", "=","backspace"],-->
    <!--["123","keyboardchange", "space","?", "return"]-->
    <!--Shift Page-->
    <Row>
        <Key
            android:codes="39"
            android:keyEdgeFlags="left"
            android:keyLabel="'" />
        <Key
            android:codes="42236"
            android:keyLabel="ꓼ" />
        <Key
            android:codes="42235"
            android:keyLabel="ꓱ" />
        <Key
            android:codes="42212"
            android:keyLabel="ꓤ" />
        <Key
            android:codes="42197"
            android:keyLabel="ꓕ" />
        <Key
            android:codes="42235"
            android:keyLabel="ꓻ" />
        <Key
            android:codes="42229"
            android:keyLabel="ꓵ" />
        <Key
            android:codes="42238"
            android:keyLabel="꓾" />
        <Key
            android:codes="717"
            android:keyLabel="ˍ" />
        <Key
            android:codes="42194"
            android:keyEdgeFlags="right"
            android:keyLabel="ꓒ" />
    </Row>

    <Row>
        <Key
            android:codes="42223"
            android:horizontalGap="5%p"
            android:keyEdgeFlags="left"
            android:keyLabel="ꓯ" />
        <Key
            android:codes="42232, 42236"
            android:keyLabel="ꓸꓼ" />
        <Key
            android:codes="42231"
            android:keyLabel="ꓷ" />
        <Key
            android:codes="42206"
            android:keyLabel="ꓞ" />
        <Key
            android:codes="42216"
            android:keyLabel="ꓨ" />
        <Key
            android:codes="42234"
            android:keyLabel="ꓺ" />
        <Key
            android:codes="42217"
            android:keyLabel="ꓩ" />
        <Key
            android:codes="42200"
            android:keyLabel="ꓘ" />
        <Key
            android:codes="42230"
            android:keyEdgeFlags="right"
            android:keyLabel="ꓶ" />
    </Row>

    <Row>
        <Key
            android:codes="@integer/keycode_shift"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/ic_keyboard_arrow_up"
            android:keyWidth="15%p" />
        <Key
            android:codes="42233"
            android:keyLabel="ꓹ" />
        <Key
            android:codes="58"
            android:keyLabel=":" />
        <Key
            android:codes="42203"
            android:keyLabel="ꓛ" />
        <Key
            android:codes="42213"
            android:keyLabel="ꓥ" />
        <Key
            android:codes="42221"
            android:keyLabel="ꓭ" />
        <Key
            android:codes="45"
            android:keyLabel="-" />
        <Key
            android:codes="61"
            android:keyLabel="=" />
        <Key
            android:codes="@integer/keycode_delete"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_backspace"
            android:keyWidth="15%p" />
    </Row>
    <Row
        android:keyHeight="@dimen/bottom_keys_height"
        android:rowEdgeFlags="bottom">
        <Key
            android:codes="@integer/keycode_mode_change"
            android:keyEdgeFlags="left"
            android:keyLabel="123"
            android:keyWidth="15%p" />
        <Key
            android:codes="@integer/switch_next_keyboard_key_code"
            android:keyIcon="@drawable/ic_language"
            android:keyWidth="10%p" />
        <Key
            android:codes="32"
            android:isRepeatable="true"
            android:keyIcon="@drawable/ic_space"
            android:keyWidth="50%p" />
        <Key
            android:codes="63"
            android:keyLabel="?"
            android:keyWidth="10%p" />
        <Key
            android:codes="@integer/keycode_newline"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/ic_keyboard_return"
            android:keyWidth="15%p" />
    </Row>
</Keyboard>